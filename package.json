{
    "name": "dragon-age-character-creator",
    "version": "1.0.0",
    "description": "Web-based character creator for the Dragon AGE tabletop RPG",
    "main": "server/server.js",
    "scripts": {
      "start": "node server/server.js",
      "server": "nodemon server/server.js",
      "client": "npm start --prefix client",
      "dev": "concurrently \"npm run server\" \"npm run client\"",
      "build": "npm run build --prefix client",
      "test": "jest"
    },
    "dependencies": {
      "express": "^4.17.1",
      "mongoose": "^6.0.12",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-scripts": "4.0.3",
      "redux": "^4.1.1",
      "react-redux": "^7.2.5",
      "axios": "^0.24.0",
      "jspdf": "^2.4.0",
      "bootstrap": "^5.1.3"
    },
    "devDependencies": {
      "nodemon": "^2.0.14",
      "concurrently": "^6.3.0",
      "jest": "^27.3.1",
      "@babel/core": "^7.16.0",
      "@babel/preset-env": "^7.16.0",
      "@babel/preset-react": "^7.16.0",
      "webpack": "^5.64.0",
      "webpack-cli": "^4.9.1"
    },
    "engines": {
      "node": ">=14.0.0"
    },
    "author": "ThatRasputin",
    "license": "MIT"
  }